{
    "name": "syyongx/phspring-demo",
    "description": "The demo of phspring framework",
    "keywords": [
        "PhSpring-demo"
    ],
    "homepage": "https://github.com/syyongx/PhSpring-demo",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "syyong.x",
            "email": "syyong.x@gmail.com",
            "homepage": "https://github.com/syyongx",
            "role": "Founder and project lead"
        }
    ],
    "support": {
        "issues": "https://github.com/syyongx/PhSpring-demo/issues?state=open",
        "wiki": "https://github.com/syyongx/PhSpring-demo/wiki/",
        "source": "https://github.com/syyongx/PhSpring-demo"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replace": {
        "syyongx/PhSpring": "self.version"
    },
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        },
        "PhSpring": {
            "type": "vcs",
            "url": "https://github.com/syyongx/PhSpring.git"
        }
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/"
        }
    },
    "config": {
        "process-timeout": 1800,
        "vendor-dir": "./vendor/"
    },
    "extra": {
    }
}
